{
  "name": "nestjs-demo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -P tsconfig.server.json -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "next build && npx cross-env NODE_ENV=production node dist/server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "start:w": "nodemon dist/server.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^9.2.1",
    "@types/mongoose": "^5.5.18",
    "@types/react-redux": "^7.1.4",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.14.0",
    "@zeit/next-sass": "^1.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "mongoose": "^5.7.1",
    "nest-next-module": "^0.2.3",
    "next": "^9.0.6",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-critical-css": "^2.1.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "styled-jsx": "^3.2.2",
    "ts-node-dev": "^1.0.0-pre.43"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.5.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.7.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/supertest": "^2.0.8",
    "concurrently": "^4.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.1.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "wait-on": "^3.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
